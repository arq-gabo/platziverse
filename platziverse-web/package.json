{
  "name": "platziverse-web",
  "version": "1.0.0",
  "description": "A Simple IoT Platform - Web Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "prestart": "npm run build",
    "start-dev": "DEBUG=platziverse:* nodemon server.js",
    "prestart-dev": "npm run build",
    "build": "browserify -t babelify -t vueify client/app.js -o public/bundle.js",
    "lint": "standard"
  },
  "author": "Jos√© Gabriel Guerra",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "nodemon": "^2.0.7",
    "standard": "^16.0.3",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "chart.js": "^2.6.0",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-asyncify": "^1.0.1",
    "moment": "^2.29.1",
    "platziverse-agent": "file:../platziverse-agent",
    "random-material-color": "^1.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "socket.io": "^3.0.5",
    "socket.io-client": "^3.0.5",
    "vue": "^2.4.2",
    "vue-chartjs": "^2.8.1",
    "vue-template-compiler": "^2.4.2",
    "win-node-env": "^0.4.0"
  }
}
